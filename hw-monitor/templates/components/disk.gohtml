<div class='disk-data'>
  <table class='table table-striped table-hover table-sm table-dark'>
    <tbody>
      <tr>
        <td>Total disk space:</td>
        <td>{{.total}}</td>
      </tr>
      <tr>
        <td>Used disk space:</td>
        <td>{{.used}}</td>
      </tr>
      <tr>
        <td>Free disk space:</td>
        <td>{{.free}}</td>
      </tr>
      <tr>
        <td>Percentage disk space usage:</td>
        <td>{{.usedPercent}}%</td>
      </tr>
    </tbody>
  </table>
</div>
